<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mdt="http://materialdesigninxaml.net/winfx/xaml/themes">
    <Style x:Key="BackButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource BackButtonTemplate}" />
    </Style>

    <Style x:Key="AddImageButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource AddImageButtonTemplate}" />
    </Style>

    <Style x:Key="RemoveImageButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource RemoveImageButtonTemplate}" />
    </Style>

    <Style x:Key="ToFindButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource ToFindButtonTemplate}" />
    </Style>

    <Style x:Key="AddButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource AddButtonTemplate}" />
    </Style>

    <Style x:Key="ChangeButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource ChangeButtonTemplate}" />
    </Style>

    <Style x:Key="RemoveButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource RemoveButtonTemplate}" />
    </Style>

    <Style x:Key="LockButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource LockButtonTemplate}" />
    </Style>

    <Style x:Key="ShowButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource ShowButtonTemplate}" />
    </Style>

    <Style x:Key="CopyButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource CopyButtonTemplate}" />
    </Style>

    <Style x:Key="ConsiderButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource ConsiderButtonTemplate}" />
    </Style>

    <Style x:Key="DetailsButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource DetailsButtonTemplate}" />
    </Style>

    <Style x:Key="BreakButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource BreakButtonTemplate}" />
    </Style>

    <Style x:Key="OpenInBrowserButtonStyle" TargetType="Button">
        <Setter Property="Template" Value="{StaticResource OpenInBrowserButtonTemplate}" />
    </Style>
</ResourceDictionary>